{
  "name": "calen",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "deploy": "vercel --prod",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:check": "npx eslint",
    "lint:write": "npx eslint --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "auth:generate": "npx @better-auth/cli generate --config ./libs/auth/index.ts --output ./libs/db/schemas/auth-table.ts --y",
    "db:reset": "npx tsx ./src/lib/server/scripts/reset-db.ts --url $DATABASE_URL",
    "db:generate": "cross-env NODE_ENV=development drizzle-kit generate",
    "db:migrate": "cross-env NODE_ENV=development drizzle-kit migrate",
    "db:studio": "cross-env NODE_ENV=development drizzle-kit studio",
    "db:generate:prod": "cross-env NODE_ENV=production drizzle-kit generate",
    "db:migrate:prod": "cross-env NODE_ENV=production drizzle-kit migrate",
    "db:studio:prod": "cross-env NODE_ENV=production drizzle-kit studio"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/pg": "^8.15.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.30.1",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@googleapis/calendar": "^9.8.0",
    "@googleapis/tasks": "^7.0.4",
    "@hono/zod-validator": "^0.7.0",
    "@lucide/svelte": "^0.511.0",
    "@neondatabase/serverless": "^1.0.0",
    "@tailwindcss/vite": "^4.1.7",
    "@upstash/redis": "^1.34.9",
    "@vvo/tzdb": "^6.171.0",
    "better-auth": "^1.2.8",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.35",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.0",
    "googleapi": "^1.0.2",
    "googleapis": "^149.0.0",
    "hono": "^4.7.10",
    "pg": "^8.16.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "zod": "^3.25.28"
  }
}
